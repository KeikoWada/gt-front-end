<p class="lead">Add your Favorite Place</p>
<form id="create-form">
  <div class="form-group">
    <input type="text" class="form-control" name="place[name]" id="create-name" placeholder="Name of the Place" required>
  </div>
  <div class="form-group">
    <input type="text" class="form-control" name="place[catgory]" id="create-category" placeholder="Category" required>
  </div>
  <div class="form-group">
    <input type="text" class="form-control create-address" name="place[address]" id="create-address" placeholder="Address" required>
  </div>
  <div class="form-group">
    <input type="text" class="form-control" name="place[description]" id="create-description" placeholder="Description" required>
  </div>
  <!-- <div class="form-group">
    <input type="text" class="form-control" name="place[picture]" id="create-picture" placeholder="picture">
  </div> -->
